<%= form_for @article do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %><br>
	<%= f.label :url %>
	<%= f.text_field :url %><br>
	<%= f.label :description %>
	<%= f.text_area :description %><br>
	<%= f.hidden_field :user_id, :value => current_user.id %>
	<%= f.submit value="Submit Article" %>
<% end %>
